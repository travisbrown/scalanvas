{
  "@context": {
    "@base":   "http://shelleygodwinarchive.org/",

    "sc":      "http://www.shared-canvas.org/ns/",
    "sga":     "http://www.shelleygodwinarchive.org/ns1#",
    "ore":     "http://www.openarchives.org/ore/terms/",
    "exif":    "http://www.w3.org/2003/12/exif/ns#",
    "iiif":    "http://library.stanford.edu/iiif/image-api/ns/",
    "oa":      "http://www.w3.org/ns/openannotation/core/",
    "oax":     "http://www.w3.org/ns/openannotation/extension/",
    "cnt":     "http://www.w3.org/2011/content#",
    "dc":      "http://purl.org/dc/elements/1.1/",
    "dcterms": "http://purl.org/dc/terms/",
    "dctypes": "http://purl.org/dc/dcmitype/",
    "foaf":    "http://xmlns.com/foaf/0.1/",
    "rdf":     "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs":    "http://www.w3.org/2000/01/rdf-schema#",
    "skos":    "http://www.w3.org/2004/02/skos/core#",
    "xsd":     "http://www.w3.org/2001/XMLSchema#",

    "first":  { "@id" : "rdf:first", "@type": "@id" },
    "rest":   { "@id": "rdf:rest",   "@type": "@id", "@container": "@list" },

    "width":  { "@id": "exif:width",  "@type": "xsd:int" },
    "height": { "@id": "exif:height", "@type": "xsd:int" },

    "format":           "dc:format",
    "language":         "dc:language",
    "description":      "dc:description",
    "attribution":      "sc:attributionLabel",
    "viewingDirection": "sc:viewingDirection",
    "viewingHint":      "sc:viewingHint",
    "style":            "oa:styleClass",
    "painting":         "sc:painting",
    "chars":            "cnt:chars",
    "encoding":         "cnt:characterEncoding",
    "bytes":            "cnt:bytes",
    "annotatedAt":      "oa:annotatedAt",
    "serializedAt":     "oa:serializedAt",
    "when":             "oa:when",
    "value":            "rdf:value",
    "start":            "oa:start",
    "end":              "oa:end",
    "exact":            "oa:exact",
    "prefix":           "oa:prefix",
    "suffix":           "oa:suffix",
    "label":            "rdfs:label",
    "name":             "foaf:name",
    "mbox":             "foaf:mbox",

    "license":       { "@id": "dcterms:license",          "@type": "@id" },
    "service":       { "@id": "sc:hasRelatedService",     "@type": "@id" },
    "seeAlso":       { "@id": "sc:hasRelatedDescription", "@type": "@id" },
    "within":        { "@id": "dcterms:isPartOf",         "@type": "@id" },
    "profile":       { "@id": "dcterms:conformsTo",       "@type": "@id" },

    "sequences":     { "@id": "sc:hasSequences",        "@type": "@id", "@container": "@list" },
    "canvases":      { "@id": "sc:hasCanvases",         "@type": "@id", "@container": "@list" },
    "resources":     { "@id": "sc:hasAnnotations",      "@type": "@id", "@container": "@list" },
    "images":        { "@id": "sc:hasImageAnnotations", "@type": "@id", "@container": "@list" },
    "otherContent":  { "@id": "sc:hasLists",            "@type": "@id", "@container": "@list" },
    "structures":    { "@id": "sc:hasRanges",           "@type": "@id", "@container": "@list" },
    "metadata":      { "@id": "sc:metadataLabels",      "@type": "@id", "@container": "@list"},

    "tile_height":   { "@id": "iiif:tileHeight",    "@type": "xsd:integer" },
    "tile_width":    { "@id": "iiif:tileWidth",     "@type": "xsd:integer" },
    "scale_factors": { "@id": "iiif:scaleFactor",   "@container": "@list" },
    "formats" :      { "@id": "iiif:formats",       "@container": "@list" },
    "qualities":     { "@id": "iiif:qualities",     "@container": "@list" },

    "motivation":    { "@id": "oa:motivatedBy",     "@type": "@id" },
    "resource":      { "@id": "oa:hasBody",         "@type": "@id" },
    "on":            { "@id": "oa:hasTarget",       "@type": "@id" },
    "full":          { "@id": "oa:hasSource",       "@type": "@id" },
    "selector":      { "@id": "oa:hasSelector",     "@type": "@id" },
    "stylesheet":    { "@id": "oa:styledBy",        "@type": "@id" },

    "hasState":      { "@id": "oa:hasState",        "@type": "@id" },
    "hasScope":      { "@id": "oa:hasScope",        "@type": "@id" },
    "annotatedBy":   { "@id": "oa:annotatedBy",     "@type": "@id" },
    "serializedBy":  { "@id": "oa:serializedBy",    "@type": "@id" },
    "equivalentTo":  { "@id": "oa:equivalentTo",    "@type": "@id" },
    "cachedSource":  { "@id": "oa:cachedSource",    "@type": "@id" },
    "conformsTo":    { "@id": "dcterms:conformsTo", "@type": "@id" },
    "default":       { "@id": "oa:default",         "@type": "@id" },
    "item":          { "@id": "oa:item",            "@type": "@id" },

    "beginOffset":   { "@id": "oax:begin",         "@type": "xsd:int" },
    "endOffset":     { "@id": "oax:end",            "@type": "xsd:int" },

    "textSelection": { "@id": "oax:TextOffsetSelector", "@type": "@id" }
  }
}

